<mvc:View
	controllerName="climberioui.controller.Dashboard"
	displayBlock="true"
	xmlns="sap.m"
	xmlns:mvc="sap.ui.core.mvc"
	xmlns:core="sap.ui.core"
	core:require="{
		formatter: 'climberioui/model/formatter'
	}">

	<Page
		title="{i18n>appTitle}"
		id="page"
		showHeader="false">
		<content>
			<IllustratedMessage
				title="{i18n>succeslogin}"
				illustrationType="sapIllus-SuccessHighFive"
				enableVerticalResponsiveness="true"
				description="{i18n>succeslogin}">
				<additionalContent>
				</additionalContent>
			</IllustratedMessage>
		</content>
	</Page>

</mvc:View>
