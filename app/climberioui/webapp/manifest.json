{
	"_version": "1.21.0",

	"sap.app": {
		"id": "climberioui",
		"type": "application",
		"i18n": "i18n/i18n.properties",
		"title": "{{appTitle}}",
		"description": "{{appDescription}}",
		"applicationVersion": {
			"version": "${version}"
		},
		"sourceTemplate": {
			"id": "generator-ui5-ts-app",
			"version": "1.1.0"
		},
		"dataSources": {
			"climbercapbouldering": {
				"uri": "/bouldering/",
				"type": "OData",
				"settings": {
					"localUri": "localService/metadata.xml",
					"odataVersion": "4.0"
				}
			},
			"climbercapauth": {
				"uri": "/authentication/",
				"type": "OData",
				"settings": {
					"localUri": "localService/metadata.xml",
					"odataVersion": "4.0"
				}
			},
			"dashboardModel": {
				"type": "JSON",
				"uri": "model/dashboard.json",
				"settings": {
					"mode": "TwoWay"
				}
			},
			"accountModel": {
				"type": "JSON",
				"uri": "model/account.json",
				"settings": {
					"mode": "TwoWay"
				}
			}
		}
	},

	"sap.ui": {
		"technology": "UI5",
		"icons": {},
		"deviceTypes": {
			"desktop": true,
			"tablet": true,
			"phone": true
		}
	},

	"sap.ui5": {
		"rootView": {
			"viewName": "climberioui.view.App",
			"type": "XML",
			"id": "app"
		},

		"dependencies": {
			"minUI5Version": "1.143.1",
			"libs": {
				"sap.ui.core": {},
				"sap.m": {}
			}
		},

		"handleValidation": true,

		"contentDensities": {
			"compact": true,
			"cozy": true
		},

		"models": {
			"bouldering": {
				"type": "sap.ui.model.odata.v4.ODataModel",
				"dataSource": "climbercapbouldering",
				"preload": true,
				"settings": {
					"synchronizationMode": "None",
					"operationMode": "Server",
					"autoExpandSelect": true,
					"earlyRequests": true
				}
			},
			"authentication": {
				"type": "sap.ui.model.odata.v4.ODataModel",
				"dataSource": "climbercapauth",
				"preload": true,
				"settings": {
					"synchronizationMode": "None",
					"operationMode": "Server",
					"autoExpandSelect": true,
					"earlyRequests": true
				}
			},
			"dashboard": {
				"type": "sap.ui.model.json.JSONModel",
				"dataSource": "dashboardModel"
			},
			"account": {
				"type": "sap.ui.model.json.JSONModel",
				"dataSource": "accountModel"
			},
			"i18n": {
				"type": "sap.ui.model.resource.ResourceModel",
				"settings": {
					"bundleName": "climberioui.i18n.i18n",
					"supportedLocales": ["en", "de"],
					"fallbackLocale": "en",
					"async": true
				}
			}
		},

		"routing": {
			"config": {
				"routerClass": "sap.m.routing.Router",
				"type": "View",
				"viewType": "XML",
				"path": "climberioui.view",
				"controlId": "app",
				"controlAggregation": "pages"
			},
			"routes": [
				{
					"pattern": "",
					"name": "main",
					"target": "main"
				},
				{
					"pattern": "dashboard",
					"name": "dashboard",
					"target": "dashboard"
				},
				{
					"pattern": "auth/failed",
					"name": "failedAuth",
					"target": "failedAuth"
				},
				{
					"pattern": "dashboard/home",
					"name": "home",
					"target": "home"
				},
				{
					"pattern": "dashboard/account",
					"name": "account",
					"target": "account"
				}
			],
			"targets": {
				"main": {
					"id": "main",
					"name": "Main"
				},
				"dashboard": {
					"id": "dashboard",
					"name": "Dashboard"
				},
				"home": {
					"id": "home",
					"name": "Home",
					"parent": "dashboard",
					"controlId": "dashboardContainer",
					"controlAggregation": "pages"
				},
				"account": {
					"id": "account",
					"name": "Account",
					"parent": "dashboard",
					"controlId": "dashboardContainer",
					"controlAggregation": "pages"
				},
				"failedAuth": {
					"id": "failedAuth",
					"name": "FailedAuth"
				}
			}
		}
	}
}
